{
  "openapi": "3.0.4",
  "info": {
    "title": "Notebook Execution Artifact Service",
    "description": "Store and retrieve parameters and result for notebook execution.",
    "contact": {
      "name": "NI",
      "url": "https://www.ni.com/systemlink",
      "email": "support@ni.com"
    },
    "version": "1"
  },
  "paths": {
    "/ninbartifact/v1/artifacts": {
      "post": {
        "tags": [
          "Artifact"
        ],
        "summary": "Create an artifact.",
        "operationId": "CreateArtifact",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "workspace": {
                    "type": "string",
                    "format": "string"
                  },
                  "artifact": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "workspace": {
                  "style": "form"
                },
                "artifact": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateArtifactResponse"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          }
        }
      }
    },
    "/ninbartifact/v1/artifacts/{artifactId}": {
      "get": {
        "tags": [
          "Artifact"
        ],
        "summary": "Download an artifact.",
        "parameters": [
          {
            "name": "artifactId",
            "in": "path",
            "description": "The ID of the artifact",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          }
        }
      },
      "patch": {
        "tags": [
          "Artifact"
        ],
        "summary": "Updates the TTL of an artifact.",
        "parameters": [
          {
            "name": "artifactId",
            "in": "path",
            "description": "The ID of the artifact",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "description": "The expiry in days value to be updated",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateArtifactRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateArtifactRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateArtifactRequest"
              }
            },
            "text/plain": {
              "schema": {
                "$ref": "#/components/schemas/UpdateArtifactRequest"
              }
            },
            "application/x-www-form-urlencoded": {
              "schema": {
                "$ref": "#/components/schemas/UpdateArtifactRequest"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "Update acknowledged."
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Artifact"
        ],
        "summary": "Deletes an artifact.",
        "parameters": [
          {
            "name": "artifactId",
            "in": "path",
            "description": "The ID of the artifact",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Delete acknowledged."
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BaseResponse": {
        "type": "object",
        "properties": {
          "error": {
            "$ref": "#/components/schemas/HttpError"
          }
        },
        "additionalProperties": false
      },
      "CreateArtifactResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The ID of the artifact",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "HttpError": {
        "title": "Error",
        "required": [
          "name",
          "message"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "String error code",
            "example": "NotebookExecutionService.NonExistingExecutionWithIdentifier"
          },
          "code": {
            "type": "integer",
            "description": "Numeric error code",
            "format": "int32",
            "nullable": true,
            "example": -251007
          },
          "message": {
            "type": "string",
            "description": "Complete error message",
            "example": "The execution ID does not exist or the user has no access to it."
          },
          "resourceType": {
            "type": "string",
            "description": "Type of resource associated with the error",
            "nullable": true,
            "example": "Execution"
          },
          "resourceId": {
            "type": "string",
            "description": "Identifier of the resource associated with the error",
            "nullable": true,
            "example": "04fa1537-94e4-4033-8bff-0fda285b1f06"
          },
          "args": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Positional argument values for the error code",
            "example": [
              "04fa1537-94e4-4033-8bff-0fda285b1f06"
            ]
          },
          "innerErrors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/HttpError"
            },
            "description": "Inner errors when the top-level error represents more than one error",
            "nullable": true
          }
        },
        "additionalProperties": false,
        "description": "Contains error information.",
        "example": {
          "name": "Skyline.OneOrMoreErrorsOccurred",
          "code": -251041,
          "message": "One or more errors occurred. See the contained list for details of each error.",
          "args": [ ],
          "innerErrors": [
            {
              "name": "NotebookExecutionService.NonExistingExecutionWithIdentifier",
              "code": -251007,
              "message": "The execution ID does not exist or the user has no access to it.",
              "resourceType": "Execution",
              "resourceId": "04fa1537-94e4-4033-8bff-0fda285b1f06",
              "args": [
                "04fa1537-94e4-4033-8bff-0fda285b1f06"
              ],
              "innerErrors": [ ]
            }
          ]
        }
      },
      "UpdateArtifactRequest": {
        "required": [
          "expiryInDays"
        ],
        "type": "object",
        "properties": {
          "expiryInDays": {
            "type": "integer",
            "description": "Expiry in days",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "apikey": {
        "type": "apiKey",
        "name": "X-NI-API-KEY",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "apikey": [ ]
    }
  ]
}