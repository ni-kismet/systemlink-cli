            "nullable": true
          },
          "interpretingServiceName": {
            "type": "string",
            "description": "Gets or sets the name of the interpretting service",
            "nullable": true,
            "example": "smtp"
          },
          "displayName": {
            "type": "string",
            "description": "Gets or sets the address group's display name",
            "nullable": true,
            "example": "name"
          },
          "properties": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "description": "Gets or sets the address group's properties",
            "nullable": true,
            "example": {
              "property": "value"
            }
          },
          "fields": {
            "type": "object",
            "additionalProperties": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "description": "Gets or sets the address group's fields",
            "nullable": true,
            "example": {
              "toAddresses": [
                "address1@example.com"
              ],
              "ccAddresses": [
                "address2@example.com"
              ],
              "bccAddresses": [
                "address3@example.com"
              ]
            }
          },
          "referencingNotificationStrategies": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "Gets or sets the address group's referencing notification strategies",
            "nullable": true
