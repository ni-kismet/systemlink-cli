    "title": "Resource Management",
    "description": "Resource Management Service exposes APIs to create, delete, utilize and monitor resources.",
    "contact": {
      "name": "NI",
      "url": "https://www.ni.com/systemlink",
      "email": "support@ni.com"
    },
    "version": "1"
