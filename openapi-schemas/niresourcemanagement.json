  "paths": {
    "/niresourcemanagement/v1/resource-groups": {
      "post": {
        "tags": [
          "ResourceGroups"
        ],
        "summary": "Creates resource groups",
        "description": "Creates one or more resource groups with the provided data.",
        "requestBody": {
          "description": "The parameters for creating resource groups.",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateResourceGroupsRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "The resources were partially created.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateResourceGroupsPartialSuccessResponse"
                }
              }
            }
          },
          "201": {
            "description": "The resources were created successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateResourceGroupsResponse"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HttpError"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized."
          }
        }
      }
    },
    "/niresourcemanagement/v1/resource-groups/{id}": {
