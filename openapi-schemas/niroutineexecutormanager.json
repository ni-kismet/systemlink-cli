{
  "openapi": "3.0.4",
  "info": {
    "title": "SystemLink Routine Executor",
    "description": "Executes routines that run notebooks against SystemLink resources.",
    "contact": {
      "name": "NI",
      "url": "https://www.ni.com/systemlink",
      "email": "support@ni.com"
    },
    "version": "1"
  },
  "paths": {
    "/niroutineexecutor/v1/execute": {
      "post": {
        "tags": [
          "Executor"
        ],
        "summary": "Executes a routine",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ExecutionRequest"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "OK"
          },
          "200": {
            "description": "Partial success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          }
        }
      }
    },
    "/niroutineexecutor/v1/execute-many": {
      "post": {
        "tags": [
          "Executor"
        ],
        "summary": "Executes multiple routines",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ExecutionsRequest"
              }
            }
          }
        },
        "responses": {
          "204": {
            "description": "OK"
          },
          "200": {
            "description": "Partial success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          },
          "default": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BaseResponse"
                }
              }
            }
          }
        }
      }
    },
    "/niroutineexecutor": {
      "get": {
        "tags": [
          "RootEndpoint"
        ],
        "summary": "Returns information about API versions and available operations.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Versions"
                }
              }
            }
          }
        }
      }
    },
    "/niroutineexecutor/v1": {
      "get": {
        "tags": [
          "RootEndpoint"
        ],
        "summary": "Returns information and available operations for version 1 of the API.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Version"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BaseResponse": {
        "type": "object",
        "properties": {
          "error": {
            "$ref": "#/components/schemas/HttpError"
          }
        },
        "additionalProperties": false
      },
      "ExecutionRequest": {
        "required": [
          "routineId",
          "sourceType"
        ],
        "type": "object",
        "properties": {
          "routineId": {
            "minLength": 1,
            "type": "string",
            "description": "The id of the routine to be executed."
          },
          "sourceType": {
            "enum": [
              "FILE",
              "TIME"
            ],
            "type": "string",
            "description": "The source type of the routine execution."
          },
          "parameters": {
            "type": "object",
            "additionalProperties": {
              "type": "string"
            },
            "description": "The parameters of the routine executions.\nPossible values: \"fileId\" (required if sourceType=\"FILE\"), \"eventId\" (optional; used for idempotence)",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ExecutionsRequest": {
        "required": [
          "executions"
        ],
        "type": "object",
        "properties": {
          "executions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ExecutionRequest"
            },
            "description": "List of executions to be requested."
          }
        },
        "additionalProperties": false
      },
      "HttpError": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "code": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "message": {
            "type": "string",
            "nullable": true
          },
          "resourceType": {
            "type": "string",
            "nullable": true
          },
          "resourceId": {
            "type": "string",
            "nullable": true
          },
          "args": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "nullable": true
          },
          "innerErrors": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/HttpError"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Operation": {
        "type": "object",
        "properties": {
          "available": {
            "type": "boolean",
            "description": "Whether the operation is available to the caller.",
            "example": true
          },
          "version": {
            "type": "integer",
            "description": "Version of the available operation.",
            "format": "int32",
            "default": 1,
            "example": 1
          }
        },
        "additionalProperties": false,
        "description": "Operation information"
      },
      "Operations": {
        "type": "object",
        "properties": {
          "execute": {
            "$ref": "#/components/schemas/Operation"
          }
        },
        "additionalProperties": false,
        "description": "Available operations in the V1 version of the API."
      },
      "Version": {
        "type": "object",
        "properties": {
          "operations": {
            "$ref": "#/components/schemas/Operations"
          }
        },
        "additionalProperties": false,
        "description": "Information about the v1 version of the API."
      },
      "Versions": {
        "type": "object",
        "properties": {
          "version": {
            "type": "string",
            "description": "Implementation version of the web service.",
            "nullable": true,
            "example": "v1"
          },
          "v1": {
            "$ref": "#/components/schemas/Version"
          }
        },
        "additionalProperties": false,
        "description": "Version information."
      }
    },
    "securitySchemes": {
      "apikey": {
        "type": "apiKey",
        "name": "X-NI-API-KEY",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "apikey": [ ]
    }
  ]
}